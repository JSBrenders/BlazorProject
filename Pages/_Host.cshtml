@page "/"
@namespace ProjetTp1.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>ProjetTp1</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link rel="stylesheet" href="/css/Custom.css" />
    <link rel="stylesheet" href="/css/noUiSlider/nouislider.css" />
    <link href="_content/Blazored.Typeahead/blazored-typeahead.css" rel="stylesheet" />
    <link href="_content/Blazored.Toast/blazored-toast.css" rel="stylesheet" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="css/Chess.css" rel="stylesheet" />
    <style id="scopedCss"></style>
</head>
<body id="body">
    <app>
        <component type="typeof(App)" render-mode="ServerPrerendered" />
    </app>

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="/lib/jquery/jquery.min.js"></script>
    <script src="/lib/jqueryui/jquery-ui.min.js"></script>
    <script src="css/noUiSlider/nouislider.js"></script>
    <script src="_framework/blazor.server.js"></script>
    <script src="_content/Blazored.Typeahead/blazored-typeahead.js"></script>
    <script src="/_content/C1.Blazor.Input/scripts.js"></script>
    <script src="/_content/BlazorScopedCss/jsInterop.js"></script>
    <script src="/js/Custom.js"></script>
    <script src="/js/Chess.js"></script>
    <script src="/lib/lodash.js/lodash.min.js"></script>
    <script src="_content/Microsoft.AspNetCore.Components.Web.Extensions/headManager.js"></script>
</body>
</html>
